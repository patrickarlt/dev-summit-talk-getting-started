<?xml version="1.0" encoding="utf-8" ?>

<ProxyConfig allowedReferers="*" logFile="proxy_log.log" mustMatch="true">
  <serverUrls>
    <serverUrl
      url="http://traffic.arcgis.com"
      matchAll="true"
      oauth2Endpoint="https://www.arcgis.com/sharing/oauth2"
      clientId="XXX"
      clientSecret="XXX"
      rateLimit="120"
      rateLimitPeriod="60">
    </serverUrl>
    <serverUrl
      url="http://route.arcgis.com"
      matchAll="true"
      oauth2Endpoint="https://www.arcgis.com/sharing/oauth2"
      clientId="XXX"
      clientSecret="XXX"
      rateLimit="120"
      rateLimitPeriod="60">
    </serverUrl>
  </serverUrls>
</ProxyConfig>

